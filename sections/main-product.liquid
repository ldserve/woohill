<script>
  var sizeObj = {% if product.metafields.my_fields.sizeobj %} {{ product.metafields.my_fields.sizeobj }} {% else %} 0 {% endif %};
  if (sizeObj) {
    var sizeGuideHtml =
        `<div class="size-guide-fixed">
            <div class="size-guide-wrap">
              <div class="size-guide-title">SIZE GUIDE <div class="size-close"><svg t="1642408377022" class="icon" viewBox="0 0 1024 1024" version="1.1" xmlns="http://www.w3.org/2000/svg" p-id="4004" width="26" height="26"><path d="M512 1024C229.220848 1024 0 794.779152 0 512S229.220848 0 512 0 1024 229.220848 1024 512 794.779152 1024 512 1024z m0-959.984485C264.564364 64.015515 63.984485 264.564364 63.984485 512c0 247.435636 200.579879 447.984485 448.015515 447.984485S960.015515 759.435636 960.015515 512c0-247.435636-200.579879-447.984485-448.015515-447.984485z m45.428364 448.170667l135.602424 135.602424a31.992242 31.992242 0 0 1-45.273212 45.242182l-135.602424-135.602424-136.471273 136.533333a32.271515 32.271515 0 0 1-45.614546-45.614545l136.502303-136.533334-135.602424-135.602424a31.992242 31.992242 0 1 1 45.273212-45.242182l135.602424 135.602424 137.495273-137.526303a32.271515 32.271515 0 1 1 45.614546 45.614546l-137.526303 137.526303z" p-id="4005" fill="#ffffff"></path></svg></div></div>
              <div class="size-guide-name">SIZE CHART</div>
              <div class="size-content"><div class="size-select-wrap"><div class="size-select selected" data-select="inch">INCH</div><div class="size-select" data-select="cm">CM</div></div><div class="size-detail-wrap">`;
    var html = {};
    var inchHtml = '<div class="size-detail selected" data-content="inch"><table>';
    var cmHtml = '<div class="size-detail" data-content="cm"><table>';

    function addTableHtml(list, html) {
      var thisHtml = "";
      var thisHtmlfixed = '<table class="table-fixed">';
      list.forEach((v, i) => {
        if (i == 0) {
          thisHtml += '<thead><tr>';
        } else {
          thisHtml += '<tr>';
        }
        thisHtmlfixed += '<tr>';
        v.forEach((value, index) => {
          if (index == 0) {
            thisHtml += '<td class="first-column">' + value + '</td>';
            thisHtmlfixed += '<td class="first-column">' + value + '</td>';
          } else {
            thisHtml += '<td>' + value + '</td>';
          }
        })
        if (i == 0) {
          thisHtml += '</thead></tr>';
        } else {
          thisHtml += '</tr>';
        }
        thisHtmlfixed += '</tr>';
      });

      thisHtml += '</table>';
      thisHtmlfixed += '</table>';
      if (html == inchHtml) {
        inchHtml += (thisHtml + thisHtmlfixed);
      } else if (html == cmHtml) {
        cmHtml += (thisHtml + thisHtmlfixed);
      }
      ;
    }

    addTableHtml(sizeObj.inchList, inchHtml);
    addTableHtml(sizeObj.cmList, cmHtml);
    inchHtml += '</div>';
    cmHtml += '</div>';
    sizeGuideHtml += (inchHtml + cmHtml);
    sizeGuideHtml +=
        `</div>
          <div class='size-tips'>Short = 5'3 - 5'7 1/2", Regular = 5'8 - 6' 1/2", Tall = 6'1- 6'3", Tall sizes have 2" added to body and sleeve length. *Arm length refers to your actual arm length,not the garment's sleeve length. </div>
          </div>
          <div class="size-guide-name">SIZING INSTRUCTIONS</div>
          <div class="size-guide-PC-image"><img src="https://cdn.shopify.com/s/files/1/0619/9682/6882/files/PC.jpg?v=1642407678" alt=""></div>
          <div class="size-guide-M-image"><img src="https://cdn.shopify.com/s/files/1/0619/9682/6882/files/M.jpg?v=1642407678" alt=""></div>
          </div>
          </div>`;
    document.querySelector("body").insertAdjacentHTML('beforeEnd', sizeGuideHtml);
    document.querySelector(".sizeGuideBtn").addEventListener("click", function () {
      document.querySelector(".size-guide-fixed").style.display = "block";
      document.body.classList.add('overflow-hidden');
    });
    const sizes = document.querySelectorAll(".size-select-wrap .size-select")
    sizes.forEach((item) => {
      item.onclick = () => {
        var select = item.getAttribute("data-select");
        sizes.forEach((i) => {
          i.classList.remove('selected')
        })
        document.querySelectorAll(".size-detail").forEach((el) => {
          el.classList.remove("selected");
        })
        document.querySelector('[data-select=' + select + ']').classList.add("selected");
        document.querySelector('[data-content=' + select + ']').classList.add("selected");
      };
    })
    document.querySelector(".size-guide-fixed").onclick = function () {
      document.querySelector(".size-guide-fixed").style.display = "none";
      document.body.classList.remove('overflow-hidden');
    };
    document.querySelector(".size-close").onclick = function () {
      document.querySelector(".size-guide-fixed").style.display = "none";
      document.body.classList.remove('overflow-hidden');
    };
    document.querySelector(".size-guide-wrap").onclick = function (e) {
      e.preventDefault();
      e.stopPropagation();
    };
  }
</script>
