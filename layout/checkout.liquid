  <body>
    <style>
      .pp-message {
        margin-bottom: .5em;
      }
    
      .info__text {
        color: black;
        text-align: right;
      }
    
      .alert_text {
        font-weight: 600;
      }
    
      .payment-list__item {
        width: 38px;
        height: 24px;
        margin: 4px;
        background-color: #FFF;
      }
    
      .payment-list {
        margin-top: 10px;
      }
      .info-coupon-code{
        margin: 0 0 1rem;
        font-size: 1rem;
      }
      @media screen and (max-width:649px) {
        .order-summary__section--discount{
          padding-top: 0px;
        }
      }
      .btn{
      text-transform: uppercase !important;
      padding: 0 30px;
      font-size: 17px;
      line-height: 48px;
      }
    </style>

    {{ skip_to_content_link }}

    <header class="banner" data-header role="banner">
      <div class="wrap">
        {{ content_for_logo }}
      </div>
    </header>

    {{ order_summary_toggle }}
    <div class="content" data-content>
      <div class="wrap">
        <div class="main">
          <header class="main__header" role="banner">
            {{ content_for_logo }}
            {{ breadcrumb }}
            {{ alternative_payment_methods }}
          </header>
          <main class="main__content" role="main">
            {{ content_for_layout }}
          </main>
          <footer class="main__footer" role="contentinfo">
            {{ content_for_footer }}
          </footer>
        </div>
        <aside class="sidebar" role="complementary">
          <div class="sidebar__header">
            {{ content_for_logo }}
          </div>
          <div class="sidebar__content">
            {{ content_for_order_summary }}
          </div>
        </aside>
      </div>
    </div>
    {{ tracking_code }}
    {% comment %}巴西的门牌号必填{% endcomment %}
    {% if settings.brazil_house_number %}
        <script src="{{ 'brazil_house_number.js' | asset_url }}" defer></script>
    {% endif %}
    <script src="{{ 'thank_you_track_link.js' | asset_url }}" defer></script>
